# Hybserv ./compat Makefile
# $Id: Makefile.in 1305 2005-09-28 10:32:31Z kreator $

AR = @AR@
ARFLAGS = ru
RANLIB = @RANLIB@
CC = @CC@
CFLAGS = @CFLAGS@
CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
LDFLAGS = @LDFLAGS@
INCLUDEDIR = ../include
COMPATDIR = ../compat
INCLUDE = -I. -I$(INCLUDEDIR) $(CPPFLAGS)
DEFS =
LIBS = @LIBS@
MKDEP = @MKDEP@
RM = @RM@ -f

# Sources, etc.

LIBOBJS = @LIBOBJS@

OBJECTS = $(LIBOBJS)

# Binaries

LIBCOMPAT = libmycompat.a

all: $(LIBCOMPAT)

$(LIBCOMPAT): $(LIBOBJS)
	$(AR) $(ARFLAGS) $@ $(LIBOBJS)
	$(RANLIB) $@

clean:
	$(RM) $(OBJECTS) $(LIBCOMPAT)

distclean: clean
	$(RM) Makefile

install:
	@echo "Run make install from top directory!"

.SUFFIXES: .c .o

.c.o:
	$(CC) $(INCLUDE) $(CFLAGS) -c $<

# End of compat/Makefile
