CC = @CC@
CFLAGS = @CFLAGS@
RM = @RM@ -f
AR = @AR@
CHMOD = @CHMOD@
RANLIB = @RANLIB@
LS = @LS@ -l

LIB = libString.a

SOURCES =             \
  Snprintf.c          \
  Strncpy.c           \
  version.c

OBJECTS = ${SOURCES:.c=.o}

all: static

static: $(LIB)

$(LIB): $(OBJECTS)
	$(AR) rv $(LIB) $(OBJECTS)
	-@$(CHMOD) 644 $(LIB)
	-@$(RANLIB) $(LIB)
	-@$(LS) $(LIB)

clean:
	$(RM) $(LIB) $(OBJECTS)

distclean: clean
	$(RM) Makefile

.SUFFIXES: .c .o

.c.o:
	$(CC) $(CFLAGS) -c $<

# End of libString/Makefile
